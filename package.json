{
	"name": "voodoo",
	"displayName": "Voodoo",
	"description": "A dark, supernatural and neon-inspired theme",
	"version": "0.11.0",
	"publisher": "liamsheppard",
	"scripts": {
		"build": "node ./builder build-theme",
		"build-theme": "node ./builder build-theme"
	},
	"galleryBanner": {
		"color": "#25273e",
		"theme": "dark"
	},
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.12.0"
	},
	"bugs": {
		"url": "https://github.com/liamsheppard/voodoo-theme/issues"
	},
	"homepage": "https://github.com/liamsheppard/voodoo-theme/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/liamsheppard/voodoo-theme"
	},
	"categories": [
		"Themes"
	],
	"main": "builder/vscode-extension.js",
	"activationEvents": [
		"workspaceContains:**/Voodoo-color-source.json"
	],
	"contributes": {
		"jsonValidation": [
			{
				"fileMatch": "**/Voodoo-color-source.json",
				"url": "./themes/Voodoo-color-source.schema.json"
			}
		],
		"themes": [
			{
				"label": "Voodoo",
				"uiTheme": "vs-dark",
				"path": "./themes/Voodoo-color-theme.json"
			}
		]
	},
	"devDependencies": {
		"strip-json-comments": "^3.1.0",
		"vsce": "^1.40.0"
	}
}